<%= render partial: 'templates/template', locals: { book: @book , user: @currentUser} %>

<!-- Usersテーブル -->
  <table>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>

    <!--Userを取り出す-->
    <% @users.each do |user| %>
      <tr>
        <td>
        <% if user.profile_image.attached? %>
          <%= image_tag user.profile_image, size: "80x80" %>
        <% else %>
          <%= image_tag 'no_image', size: "80x80" %>
        <% end %>
        </td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
      </tr>
    <% end %>
  </table>